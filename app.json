{
  "expo": {
    "name": "alertera",
    "slug": "alertera",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/alarm.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/alarm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.univera.alertera"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/alarm.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.univera.alertera",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ],
      "notification": {
        "sound": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/alarm.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-notifications",
        {
          "sounds": [
            "./src/assets/sounds/atomne.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3f20ff6b-41f1-4f70-8d25-5b5a14c81be5"
      }
    }
  }
}